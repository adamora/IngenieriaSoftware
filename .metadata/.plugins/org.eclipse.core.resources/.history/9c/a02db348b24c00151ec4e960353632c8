#include "../src/Agenda.h"
#include "../gtest/gtest.h"

using namespace std;
using namespace Dentista;

TEST(Agenda, getYsetNumPacientes){
	Agenda a;
	a.setNumPacientes(2);
	EXPECT_EQ(2, a.getNumPacientes());
}
