#include "Contacto.h"
#include "gtest/gtest.h"

using namespace std;
using namespace Dentista;

TEST(Contacto, getYsetNombre){
	Contacto c;
	c.setNombre("Prueba");
	EXPECT_EQ("Prueba", c.getNombre());
}

TEST(CONTACTO, getYsetApellidos){
	Contacto c;
	c.setApellidos("Ambos Apellidos");
	EXPECT_EQ("Ambos Apellidos", c.getNombre());
}
